package com.juanwan11_magicnation;
import net.minecraft.nbt.NBTTagCompound;
import net.minecraft.world.World;
import net.minecraft.world.WorldSavedData;
import net.minecraft.world.storage.MapStorage;

public class MagicnationWorldSaveData extends WorldSavedData {

	
	public static boolean alreadyStarterStructure = false;
	
	
	public MagicnationWorldSaveData(String tagName) {
		super(tagName);
	}

	@Override
	public void readFromNBT(NBTTagCompound nbt) {
		
		
	}

	@Override
	public void writeToNBT(NBTTagCompound nbt) {
		
		nbt.setBoolean("alreadyStarterStructure", false);

	}

}